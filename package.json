		"@sanity/image-url": "^2.0.2",
		"@sanity/vision": "^5.0.1",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"cmdk": "^1.1.1",
		"date-fns": "^4.1.0",
		"embla-carousel-react": "^8.6.0",
		"gsap": "^3.14.2",
		"hamo": "1.0.0-dev.6",
		"input-otp": "^1.4.2",
		"lenis": "^1.3.16",
		"next": "16.0.10",
		"next-sanity": "^12.0.1",
		"next-themes": "^0.4.6",
		"react": "19.2.1",
		"react-day-picker": "^9.12.0",
